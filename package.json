{
  "name": "storybook-vrt-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir docs/storybook",
    "test": "vitest --project=unit",
    "test:watch": "vitest --project=unit --watch",
    "test:storybook": "vitest --project=storybook",
    "test:all": "vitest",
    "test:interaction": "test-storybook",
    "test:vrt": "test-storybook --url http://localhost:6006"
  },
  "dependencies": {
    "@storybook/test": "^8.6.14",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/nextjs": "^9.1.3",
    "@storybook/nextjs-vite": "^9.1.3",
    "@storybook/test-runner": "^0.23.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser-playwright": "^4.0.6",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-storybook": "^9.1.3",
    "jest-image-snapshot": "^6.4.0",
    "jsdom": "^27.1.0",
    "playwright": "^1.56.1",
    "storybook": "^9.1.3",
    "typescript": "^5",
    "vite": "^6.0.5",
    "vitest": "^4.0.6"
  }
}
